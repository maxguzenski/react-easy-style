{
  "name": "react-easy-style",
  "version": "0.2.1",
  "description": "A tiny library to easy apply css/styles to react components",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxguzenski/react-easy-style.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "css",
    "inline",
    "style",
    "styles"
  ],
  "author": "Maximiliano Guzenski <maxguzenski@gmail.com> (http://github.com/maxguzenski)",
  "homepage": "https://github.com/maxguzenski/react-easy-style",
  "bugs": "https://github.com/maxguzenski/react-easy-style/issues",
  "scripts": {
    "build": "babel --stage 0 src/ -d lib/",
    "test": "mocha  --compilers js:babel/register --recursive",
    "test:watch": "mocha  --compilers js:babel/register --recursive --watch",
    "prepublish": "rm -Rf lib/ && npm run build"
  },
  "license": "MIT",
  "dependencies": {
    "classnames": "~2.1.3"
  },
  "devDependencies": {
    "babel": "^5.5.8",
    "babel-core": "5.8.22",
    "babel-loader": "^5.1.4",
    "expect": "latest",
    "jsdom": "^6.1.0",
    "mocha": "latest",
    "mocha-jsdom": "^1.0.0",
    "react": "0.13.3"
  }
}
